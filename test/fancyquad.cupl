COMMENT	PERFORM REPEATED QUADRATIC-EQUATION SOLUTIONS
	READ N
	PERFORM QUADSOL N TIMES
	STOP
QUADSOL	BLOCK
COMMENT	SOLVE THE QUADRATIC EQUATION AX**2 + BX + C = 0
	READ A,B,C
	WRITE A,B,C
	LET D = B*B - 4*A*C
	IF D LT 0 THEN GO TO OTHER
	LET E = SQRT(D)
	LET X1 = (-B+E)/(2*A)
	LET X2 = (-B-E)/(2*A)
	WRITE X1,X2
	GO TO LAST
OTHER	LET E = SQRT(-D)
	LET XREAL = -B/(2*A)
	LET XIMAG = E/(2*A)
	WRITE XREAL,XIMAG
	STOP
QUADSOL	END

*DATA	N=4
	A=1, B=1,C=-6
	A=1,B=2,C=5
	A=-3,B=0.25,C=5
	A=3.14159,B=2.71828,C=1.4142
