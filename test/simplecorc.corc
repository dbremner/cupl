NOTE	THE SAME OLD QUADRATIC EQUATION SOLVER
	READ A, B, C
	LET DISC = B*B - 4*A*C
	LET REALPART = - B/(2*A)
	LET IMAGPART = SQRT(ABS(DISC))/(2*A)
	IF DISC LSS 0
			THEN GO TO IMAG
			ELSE GO TO REAL
IMAG	WRITE REALPART, IMAGPART
	STOP
REAL	LET ROOT1 = REALPART + IMAGPART
	LET ROOT2 = REALPART - IMAGPART
	WRITE ROOT1, ROOT2
	STOP

*DATA	A=1,B=-5,C=2.25
