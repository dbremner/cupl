COMMENT	SOLVE THE QUADRATIC EQUATION AX**2 + BX + C = 0
	READ A,B,C
	LET D = B*B - 4*A*C
	IF D LT 0 THEN GO TO OTHER
	LET E = SQRT(D)
	LET X1 = (-B+E)/(2*A)
	LET X2 = (-B-E)/(2*A)
	WRITE X1,X2
	GO TO LAST
OTHER	LET E = SQRT(-D)
	LET XREAL = -B/(2*A)
	LET XIMAG = E/(2*A)
	WRITE XREAL,XIMAG
	STOP

*DATA	A=1,B=4,C=8
