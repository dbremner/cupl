NOTE	SOLVE THE QUADRATIC EQUATION AX**2 + BX + C = 0
	READ N
	REPEAT QUAD N TIMES
QUAD	BEGIN
	READ A,B,C
	LET DISC = B*B - 4*A*C
	LET REALPART = B/(2*A)
	LET IMAGPART = SQRT(ABS(DISC))/(2*A)
	IF DISC LSS 0
		THEN GO TO IMAG
		ELSE GO TO REAL
IMAG	WRITE REALPART, IMAGPART
	GO TO QUAD
REAL	LET ROOT1 = REALPART + IMAGPART
	LET ROOT2 = REALPART - IMAGPART
	WRITE ROOT1, ROOT2
QUAD	END
	STOP

*DATA	N=4
	A=1, B=1,C=-6
	A=1,B=2,C=5
	A=-3,B=0.25,C=5
	A=3.14159,B=2.71828,C=1.4142
